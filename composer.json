{
  "name": "alhames/filter-bundle",
  "type": "symfony-bundle",
  "description": "Filter Bundle",
  "license": "MIT",
  "minimum-stability": "stable",
  "homepage": "https://github.com/alhames/filter-bundle",
  "keywords": ["filter", "bundle", "symfony"],
  "authors": [
    {
      "name": "Pavel Logachev",
      "email": "alhames@mail.ru",
      "homepage": "https://alhames.ru/"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-zlib": "*",
    "alhames/string": "^1.0",
    "symfony/framework-bundle": "^6.0",
    "symfony/yaml": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "^6.0",
    "symfony/phpunit-bridge": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Alhames\\FilterBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alhames\\FilterBundle\\Tests\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/alhames/filter-bundle/issues"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
